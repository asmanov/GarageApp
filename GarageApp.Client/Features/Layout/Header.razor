<MudAppBar Color="Color.Info" Fixed="true">
    <MudIconButton OnClick="@OpenMenu" Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Edge="Edge.Start" />
    <MudImage Src="images/logoGarage.png" Alt="Logo" Elevation="25" Class="rounded-lg" />
    <MudSpacer />
</MudAppBar>
<MainMenu _Open=@_open />

@code{
    private bool _open = false;

    private void OpenMenu()
    {
        _open = !_open;
    }
}